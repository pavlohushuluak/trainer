{
    "rewrites": [
      {
        "source": "/((?!.*\\.(js|css|png|jpg|jpeg|svg|woff2|ttf|ico|json|txt|map|webmanifest)$).*)",
        "destination": "/"
      }
    ],
    "redirects": [
      {
        "source": "/:path*",
        "has": [
          {
            "type": "host",
            "value": "tiertrainer24.com"
          }
        ],
        "destination": "https://www.tiertrainer24.com/:path*",
        "permanent": true
      }
    ]
  }
  